{
  "devDependencies": {
    "semantic-release": "15.13.12",
    "vuepress": "1.0.1"
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "now-build": "npm run docs:build && mv docs/.vuepress/dist dist",
    "build": "mv testfiles-*/dump.reaxc.html docs/.vuepress/public/report.html && yarn docs:build",
    "semantic-release": "semantic-release"
  },
  "license": "UNLICENSED",
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/njzjz/reacnetgenerator.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "conda/linux-64/*.tar.bz2",
              "label": "conda packages for Linux"
            },
            {
              "path": "conda/osx-64/*.tar.bz2",
              "label": "conda packages for macOS"
            },
            {
              "path": "conda/win-64/*.tar.bz2",
              "label": "conda packages for Windows (x64)"
            }
          ]
        }
      ]
    ]
  }
}
